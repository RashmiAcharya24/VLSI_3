vsim -gui work.up_down_counter
# vsim -gui work.up_down_counter 
# Start time: 16:40:54 on Jun 16,2025
# Loading work.up_down_counter
add wave -position insertpoint sim:/up_down_counter/*
force -freeze sim:/up_down_counter/clk 1 0, 0 {50000 ps} -r 100ns
force -freeze sim:/up_down_counter/reset 1 0, 0 {100000 ps} -r 200ns
force -freeze sim:/up_down_counter/up_down 1 0, 0 {150000 ps} -r 300ns
force -freeze sim:/up_down_counter/reset 1 0, 0 {100000 ps} -r 200ns
run
run
